benchmark h2o
  class org.dacapo.harness.H2O
  thread-model per_cpu
  jars "main.jar", "json-lib-2.4-jdk15.jar", "commons-beanutils-1.9.3.jar", "commons-collections-3.2.2.jar", "commons-lang-2.6.jar", "commons-logging.jar", "ezmorph-1.0.6.jar";

size default args "${SCRATCH}/dat", "slice_localization_data.csv", "drf" , "reference"
  threads 1      // per available processor
  thread-limit 32
  output stdout digest 0xa9e808d64b9be612e37f5e973a3da20caa0a5d85;


size large args "${SCRATCH}/dat", "CitiBike2014.csv", "drf" , "tripduration"
  threads 1      // per available processor
  thread-limit 32
  output stdout digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

description
  short	     "Open Source Fast Scalable Machine Learning Platform.",
  long       "H2O is an in-memory platform for distributed, scalable machine learning.",
  copyright	 "Copyright (c) H2O.ai. All rights reserved",
  license    "Apache License, Version 2.0",
  url        "https://github.com/h2oai/h2o-3",
  version    "3-jenkins-master-4559",
  threads    "Explicitly multi-threaded, scaled to available processors.";